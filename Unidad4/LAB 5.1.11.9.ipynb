    "def DígitodelaVida(a):\n",
    "    suma = 0\n",
    "    \n",
    "    while a:\n",
    "        suma += a % 10\n",
    "        a //= 10\n",
    "        \n",
    "    if suma > 9:\n",
    "        return DígitodelaVida(suma) \n",
    "    \n",
    "    return suma\n",
    "a = int(input(\"Ingresa tu fecha de cumpleaños (digitos): \"))\n",
    "\n",
    "print(DígitodelaVida(a)) \n",
    "\n"
